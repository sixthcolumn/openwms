<?xml version="1.0" encoding="utf-8"?>
<xs:schema targetNamespace="http://www.iec.ch/TC57/2011/MeterReadingsMessage"
	xmlns:tns="http://www.iec.ch/TC57/2011/MeterReadingsMessage" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:msg="http://www.iec.ch/TC57/2011/schema/message" xmlns:obj="http://iec.ch/TC57/2011/MeterReadings#"
	elementFormDefault="qualified" attributeFormDefault="unqualified"
	version="1.0.0">

	<!-- Base Message Definitions -->
	<xs:import namespace="http://www.iec.ch/TC57/2011/schema/message"
		schemaLocation="Message.xsd" />
	<!-- CIM Information Object Definition -->
	<xs:import namespace="http://iec.ch/TC57/2011/MeterReadings#"
		schemaLocation="MeterReadings.xsd" />

	<!-- PayloadType Definition -->
	<xs:complexType name="MeterReadingsPayloadType">
		<xs:sequence>
			<xs:element ref="obj:MeterReadings" />
			<xs:element name="OperationSet" type="msg:OperationSet"
				minOccurs="0" />
			<xs:element name="Compressed" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>For compressed and/or binary, uuencoded payloads
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Format" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Hint as to format of payload, e.g. XML, RDF, SVF,
						BINARY, PDF, ...</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<!-- Message Types -->
	<!-- RequestMessageType -->
	<xs:complexType name="MeterReadingsRequestMessageType">
		<xs:sequence>
			<xs:element name="Header" type="msg:HeaderType" />
			<xs:element name="Request" type="msg:RequestType"
				minOccurs="0" />
			<xs:element name="Payload" type="tns:MeterReadingsPayloadType"
				minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<!-- ResponseMessageType -->
	<xs:complexType name="MeterReadingsResponseMessageType">
		<xs:sequence>
			<xs:element name="Header" type="msg:HeaderType" />
			<xs:element name="Reply" type="msg:ReplyType" />
			<xs:element name="Payload" type="tns:MeterReadingsPayloadType"
				minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<!-- EventMessageType -->
	<xs:complexType name="MeterReadingsEventMessageType">
		<xs:sequence>
			<xs:element name="Header" type="msg:HeaderType" />
			<xs:element name="Payload" type="tns:MeterReadingsPayloadType"
				minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<!-- FaultMessageType -->
	<xs:complexType name="MeterReadingsFaultMessageType">
		<xs:sequence>
			<xs:element name="Reply" type="msg:ReplyType" />
		</xs:sequence>
	</xs:complexType>

	<xs:element name="CreateMeterReadings" type="tns:MeterReadingsRequestMessageType" />
	<xs:element name="ChangeMeterReadings" type="tns:MeterReadingsRequestMessageType" />
	<xs:element name="CancelMeterReadings" type="tns:MeterReadingsRequestMessageType" />
	<xs:element name="CloseMeterReadings" type="tns:MeterReadingsRequestMessageType" />
	<xs:element name="DeleteMeterReadings" type="tns:MeterReadingsRequestMessageType" />
	<xs:element name="CreatedMeterReadings" type="tns:MeterReadingsEventMessageType" />
	<xs:element name="ChangedMeterReadings" type="tns:MeterReadingsEventMessageType" />
	<xs:element name="CanceledMeterReadings" type="tns:MeterReadingsEventMessageType" />
	<xs:element name="ClosedMeterReadings" type="tns:MeterReadingsEventMessageType" />
	<xs:element name="DeletedMeterReadings" type="tns:MeterReadingsEventMessageType" />
	<xs:element name="MeterReadingsResponseMessage" type="tns:MeterReadingsResponseMessageType" />
	<xs:element name="MeterReadingsFaultMessage" type="tns:MeterReadingsFaultMessageType" />
	<xs:element name="MeterReadingsRequestMessage" type="tns:MeterReadingsRequestMessageType" />


</xs:schema>
