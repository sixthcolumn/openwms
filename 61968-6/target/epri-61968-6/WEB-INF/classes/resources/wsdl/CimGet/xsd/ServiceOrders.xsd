					<xs:documentation>Kind of multiplier.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="value" type="xs:float" sawsdl:modelReference="http://iec.ch/TC57/2014/CIM-schema-cim16-dcim12#MeterMultiplier.value">
				<xs:annotation>
					<xs:documentation>Multiplier value.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Names" type="m:Name" minOccurs="0" maxOccurs="unbounded" sawsdl:modelReference="http://iec.ch/TC57/2014/CIM-schema-cim16-dcim12#IdentifiedObject.Names">
				<xs:annotation>
					<xs:documentation>All names of this identified object.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
