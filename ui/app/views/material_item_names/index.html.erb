<h1>Listing material_item_names</h1>

<table>
  <thead>
    <tr>
      <th>Material item</th>
      <th>Name</th>
      <th>Name type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @material_item_names.each do |material_item_name| %>
      <tr>
        <td><%= material_item_name.material_item %></td>
        <td><%= material_item_name.name %></td>
        <td><%= material_item_name.name_type %></td>
        <td><%= link_to 'Show', material_item_name %></td>
        <td><%= link_to 'Edit', edit_material_item_name_path(material_item_name) %></td>
        <td><%= link_to 'Destroy', material_item_name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Material item name', new_material_item_name_path %>
