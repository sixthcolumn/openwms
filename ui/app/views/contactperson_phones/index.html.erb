<h1>Listing contactperson_phones</h1>

<table>
  <thead>
    <tr>
      <th>Contactperson</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contactperson_phones.each do |contactperson_phone| %>
      <tr>
        <td><%= contactperson_phone.contactperson %></td>
        <td><%= contactperson_phone.phone %></td>
        <td><%= link_to 'Show', contactperson_phone %></td>
        <td><%= link_to 'Edit', edit_contactperson_phone_path(contactperson_phone) %></td>
        <td><%= link_to 'Destroy', contactperson_phone, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contactperson phone', new_contactperson_phone_path %>
