<h1>Listing contactperson_emails</h1>

<table>
  <thead>
    <tr>
      <th>Contactperson</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contactperson_emails.each do |contactperson_email| %>
      <tr>
        <td><%= contactperson_email.contactperson %></td>
        <td><%= contactperson_email.email %></td>
        <td><%= link_to 'Show', contactperson_email %></td>
        <td><%= link_to 'Edit', edit_contactperson_email_path(contactperson_email) %></td>
        <td><%= link_to 'Destroy', contactperson_email, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contactperson email', new_contactperson_email_path %>
