<h1>Listing work_order_attachments</h1>

<table>
  <thead>
    <tr>
      <th>Work order</th>
      <th>Attachment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @work_order_attachments.each do |work_order_attachment| %>
      <tr>
        <td><%= work_order_attachment.work_order %></td>
        <td><%= work_order_attachment.attachment %></td>
        <td><%= link_to 'Show', work_order_attachment %></td>
        <td><%= link_to 'Edit', edit_work_order_attachment_path(work_order_attachment) %></td>
        <td><%= link_to 'Destroy', work_order_attachment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Work order attachment', new_work_order_attachment_path %>
