<h1>Listing work_order_schedules</h1>

<table>
  <thead>
    <tr>
      <th>Work order</th>
      <th>Time schedule</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @work_order_schedules.each do |work_order_schedule| %>
      <tr>
        <td><%= work_order_schedule.work_order %></td>
        <td><%= work_order_schedule.time_schedule %></td>
        <td><%= link_to 'Show', work_order_schedule %></td>
        <td><%= link_to 'Edit', edit_work_order_schedule_path(work_order_schedule) %></td>
        <td><%= link_to 'Destroy', work_order_schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Work order schedule', new_work_order_schedule_path %>
