<h1>Listing work_order_names</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Name type</th>
      <th>Work order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @work_order_names.each do |work_order_name| %>
      <tr>
        <td><%= work_order_name.name %></td>
        <td><%= work_order_name.name_type %></td>
        <td><%= work_order_name.work_order %></td>
        <td><%= link_to 'Show', work_order_name %></td>
        <td><%= link_to 'Edit', edit_work_order_name_path(work_order_name) %></td>
        <td><%= link_to 'Destroy', work_order_name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Work order name', new_work_order_name_path %>
