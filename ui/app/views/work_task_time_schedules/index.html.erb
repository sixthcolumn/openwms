<h1>Listing work_task_time_schedules</h1>

<table>
  <thead>
    <tr>
      <th>Work task</th>
      <th>Time schedule</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @work_task_time_schedules.each do |work_task_time_schedule| %>
      <tr>
        <td><%= work_task_time_schedule.work_task %></td>
        <td><%= work_task_time_schedule.time_schedule %></td>
        <td><%= link_to 'Show', work_task_time_schedule %></td>
        <td><%= link_to 'Edit', edit_work_task_time_schedule_path(work_task_time_schedule) %></td>
        <td><%= link_to 'Destroy', work_task_time_schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Work task time schedule', new_work_task_time_schedule_path %>
