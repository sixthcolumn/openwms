<h1>Listing contactpeople</h1>

<table>
  <thead>
    <tr>
      <th>Lastname</th>
      <th>Firstname</th>
      <th>Middle</th>
      <th>Prefix</th>
      <th>Suffix</th>
      <th>Governmentid</th>
      <th>Affiliation</th>
      <th>Primary</th>
      <th>Secondary</th>
      <th>Objectid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contactpeople.each do |contactperson| %>
      <tr>
        <td><%= contactperson.lastname %></td>
        <td><%= contactperson.firstname %></td>
        <td><%= contactperson.middle %></td>
        <td><%= contactperson.prefix %></td>
        <td><%= contactperson.suffix %></td>
        <td><%= contactperson.governmentid %></td>
        <td><%= contactperson.affiliation %></td>
        <td><%= contactperson.primary %></td>
        <td><%= contactperson.secondary %></td>
        <td><%= contactperson.objectid %></td>
        <td><%= link_to 'Show', contactperson %></td>
        <td><%= link_to 'Edit', edit_contactperson_path(contactperson) %></td>
        <td><%= link_to 'Destroy', contactperson, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contactperson', new_contactperson_path %>
