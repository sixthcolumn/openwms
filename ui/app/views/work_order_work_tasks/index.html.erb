<h1>Listing work_order_work_tasks</h1>

<table>
  <thead>
    <tr>
      <th>Work order</th>
      <th>Work task</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @work_order_work_tasks.each do |work_order_work_task| %>
      <tr>
        <td><%= work_order_work_task.work_order %></td>
        <td><%= work_order_work_task.work_task %></td>
        <td><%= link_to 'Show', work_order_work_task %></td>
        <td><%= link_to 'Edit', edit_work_order_work_task_path(work_order_work_task) %></td>
        <td><%= link_to 'Destroy', work_order_work_task, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Work order work task', new_work_order_work_task_path %>
