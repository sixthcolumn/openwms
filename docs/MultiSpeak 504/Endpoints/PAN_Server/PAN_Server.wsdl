"/></wsdl:message><wsdl:message name="InitiatePANRegistrationStatusRequestsSoapIn"><wsdl:part name="parameters" element="tns:InitiatePANRegistrationStatusRequests"/></wsdl:message><wsdl:message name="InitiatePANRegistrationStatusRequestsSoapOut"><wsdl:part name="parameters" element="tns:InitiatePANRegistrationStatusRequestsResponse"/></wsdl:message><wsdl:message name="InitiatePremisesDisplayBillingMessagesSoapIn"><wsdl:part name="parameters" element="tns:InitiatePremisesDisplayBillingMessages"/></wsdl:message><wsdl:message name="InitiatePremisesDisplayBillingMessagesSoapOut"><wsdl:part name="parameters" element="tns:InitiatePremisesDisplayBillingMessagesResponse"/></wsdl:message><wsdl:message name="InitiatePremisesDisplayCapabilitySettingsSoapIn"><wsdl:part name="parameters" element="tns:InitiatePremisesDisplayCapabilitySettings"/></wsdl:message><wsdl:message name="InitiatePremisesDisplayCapabilitySettingsSoapOut"><wsdl:part name="parameters" element="tns:InitiatePremisesDisplayCapabilitySettingsResponse"/></wsdl:message><wsdl:message name="InitiatePremisesDisplayMessagesSoapIn"><wsdl:part name="parameters" element="tns:InitiatePremisesDisplayMessages"/></wsdl:message><wsdl:message name="InitiatePremisesDisplayMessagesSoapOut"><wsdl:part name="parameters" element="tns:InitiatePremisesDisplayMessagesResponse"/></wsdl:message><wsdl:message name="InitiateThermostatConfigurationRequestsSoapIn"><wsdl:part name="parameters" element="tns:InitiateThermostatConfigurationRequests"/></wsdl:message><wsdl:message name="InitiateThermostatConfigurationRequestsSoapOut"><wsdl:part name="parameters" element="tns:InitiateThermostatConfigurationRequestsResponse"/></wsdl:message><wsdl:message name="InitiateThermostatConfigurationsSoapIn"><wsdl:part name="parameters" element="tns:InitiateThermostatConfigurations"/></wsdl:message><wsdl:message name="InitiateThermostatConfigurationsSoapOut"><wsdl:part name="parameters" element="tns:InitiateThermostatConfigurationsResponse"/></wsdl:message><wsdl:message name="InitiateThermostatScheduleRequestsSoapIn"><wsdl:part name="parameters" element="tns:InitiateThermostatScheduleRequests"/></wsdl:message><wsdl:message name="InitiateThermostatScheduleRequestsSoapOut"><wsdl:part name="parameters" element="tns:InitiateThermostatScheduleRequestsResponse"/></wsdl:message><wsdl:message name="InitiateThermostatSchedulesSoapIn"><wsdl:part name="parameters" element="tns:InitiateThermostatSchedules"/></wsdl:message><wsdl:message name="InitiateThermostatSchedulesSoapOut"><wsdl:part name="parameters" element="tns:InitiateThermostatSchedulesResponse"/></wsdl:message><wsdl:message name="InsertInPANDeviceGroupSoapIn"><wsdl:part name="parameters" element="tns:InsertInPANDeviceGroup"/></wsdl:message><wsdl:message name="InsertInPANDeviceGroupSoapOut"><wsdl:part name="parameters" element="tns:InsertInPANDeviceGroupResponse"/></wsdl:message><wsdl:message name="InsertInPremisesDisplayGroupSoapIn"><wsdl:part name="parameters" element="tns:InsertInPremisesDisplayGroup"/></wsdl:message><wsdl:message name="InsertInPremisesDisplayGroupSoapOut"><wsdl:part name="parameters" element="tns:InsertInPremisesDisplayGroupResponse"/></wsdl:message><wsdl:message name="LinkAttachmentsToObjectsSoapIn"><wsdl:part name="parameters" element="tns:LinkAttachmentsToObjects"/></wsdl:message><wsdl:message name="LinkAttachmentsToObjectsSoapOut"><wsdl:part name="parameters" element="tns:LinkAttachmentsToObjectsResponse"/></wsdl:message><wsdl:message name="PingURLSoapIn"><wsdl:part name="parameters" element="tns:PingURL"/></wsdl:message><wsdl:message name="PingURLSoapOut"><wsdl:part name="parameters" element="tns:PingURLResponse"/></wsdl:message><wsdl:message name="RemoveFromPANDeviceGroupSoapIn"><wsdl:part name="parameters" element="tns:RemoveFromPANDeviceGroup"/></wsdl:message><wsdl:message name="RemoveFromPANDeviceGroupSoapOut"><wsdl:part name="parameters" element="tns:RemoveFromPANDeviceGroupResponse"/></wsdl:message><wsdl:message name="RemoveFromPremisesDisplayGroupSoapIn"><wsdl:part name="parameters" element="tns:RemoveFromPremisesDisplayGroup"/></wsdl:message><wsdl:message name="RemoveFromPremisesDisplayGroupSoapOut"><wsdl:part name="parameters" element="tns:RemoveFromPremisesDisplayGroupResponse"/></wsdl:message><wsdl:message name="UnlinkAttachmentsFromObjectsSoapIn"><wsdl:part name="parameters" element="tns:UnlinkAttachmentsFromObjects"/></wsdl:message><wsdl:message name="UnlinkAttachmentsFromObjectsSoapOut"><wsdl:part name="parameters" element="tns:UnlinkAttachmentsFromObjectsResponse"/></wsdl:message><wsdl:portType name="PAN_ServerSoap"><wsdl:operation name="CancelPANRegistrations"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Requester asks the receiver to initiate and manage the eviction of an existing PAN device from a PAN.  The receiver publishes the results of the device registration asynchronously using a PANRegistrationNotification to the URL specified in the responseURL calling parameter and references the transactionID specified to link the transaction to this Cancel request.  Information about failed transactions is returned using the result object in the response message header.Receiver returns information about failed transactions using the result element of the response message header.</wsdl:documentation><wsdl:input message="tns:CancelPANRegistrationsSoapIn"/><wsdl:output message="tns:CancelPANRegistrationsSoapOut"/></wsdl:operation><wsdl:operation name="CancelPremisesDisplayBillingMessages"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Publisher notifies subscriber that billing messages that previously have been sent should be canceled on an premises display associated with a specific PAN interface (ESI).</wsdl:documentation><wsdl:input message="tns:CancelPremisesDisplayBillingMessagesSoapIn"/><wsdl:output message="tns:CancelPremisesDisplayBillingMessagesSoapOut"/></wsdl:operation><wsdl:operation name="CancelPremisesDisplayMessages"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Publisher notifies subscriber that messages that previously have been sent should be canceled on a premises displays associated with a PANAddressingGroup.</wsdl:documentation><wsdl:input message="tns:CancelPremisesDisplayMessagesSoapIn"/><wsdl:output message="tns:CancelPremisesDisplayMessagesSoapOut"/></wsdl:operation><wsdl:operation name="CreatePANDeviceGroups"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Sender notifies receiver that new PAN device groups have been created on the server and optionally adds PAN devices to the new groups. The groups may be empty when first established.Receiver returns information about failed transactions using the result element of the response message header.</wsdl:documentation><wsdl:input message="tns:CreatePANDeviceGroupsSoapIn"/><wsdl:output message="tns:CreatePANDeviceGroupsSoapOut"/></wsdl:operation><wsdl:operation name="CreatePremisesDisplayGroups"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Sender notifies receiver that new premises display groups have been created on the server and optionally adds premisesDisplays to the new groups.The groups may be empty when first established.Receiver returns information about failed transactions using the result element of the response message header.</wsdl:documentation><wsdl:input message="tns:CreatePremisesDisplayGroupsSoapIn"/><wsdl:output message="tns:CreatePremisesDisplayGroupsSoapOut"/></wsdl:operation><wsdl:operation name="DeletePANDeviceGroups"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Sender notifies receiver that Sender has deleted previously created PAN device groups.Receiver returns information about failed transactions using the result element of the response message header.</wsdl:documentation><wsdl:input message="tns:DeletePANDeviceGroupsSoapIn"/><wsdl:output message="tns:DeletePANDeviceGroupsSoapOut"/></wsdl:operation><wsdl:operation name="DeletePremisesDisplayGroups"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Sender notifies receiver that Sender has deleted previously established premises display groups, specified by sending a list of premisesDisplayGroupIDs.Receiver returns information about failed transactions using the result element of the response message header.</wsdl:documentation><wsdl:input message="tns:DeletePremisesDisplayGroupsSoapIn"/><wsdl:output message="tns:DeletePremisesDisplayGroupsSoapOut"/></wsdl:operation><wsdl:operation name="GetAttachmentsByObjectRefs"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Requests an array of attachments associated with specific objects, given their objectRefs.</wsdl:documentation><wsdl:input message="tns:GetAttachmentsByObjectRefsSoapIn"/><wsdl:output message="tns:GetAttachmentsByObjectRefsSoapOut"/></wsdl:operation><wsdl:operation name="GetDomainMembers"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">The client requests from the server the members of a specific domain of information, identified by the domainName parameter, which are supported by the server. This method is used, along with the GetDomainNames method to enable systems to exchange information about application-specific or installation-specific lists of information, such as the lists of counties for this installation or the list of serviceStatusCodes used by the server.Responder returns information about failed transactions using the result element of the response message header.</wsdl:documentation><wsdl:input message="tns:GetDomainMembersSoapIn"/><wsdl:output message="tns:GetDomainMembersSoapOut"/></wsdl:operation><wsdl:operation name="GetDomainNames"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">The client requests from the server a list of names of domains supported by the server. This method is used, along with the GetDomainMembers method to enable systems to exchange information about application-specific or installation-specific lists of information, such as the lists of counties for this installation or the list of serviceStatusCodes used by the server. It is recommended that domain names be returned in the form of the name of a named object (noun) in the MultiSpeak schema dotted with the field name of interest. For instance, if the system of record is returningworkFlowStatus identifiers that would be used on designs, it is suggested that the domain name be called design.workflowStatus, using the same spelling and capitalization used in the MultiSpeak schema.Responder returns information about failed transactions using the result element of the response message header.</wsdl:documentation><wsdl:input message="tns:GetDomainNamesSoapIn"/><wsdl:output message="tns:GetDomainNamesSoapOut"/></wsdl:operation><wsdl:operation name="GetMethods"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Requester requests a list of methods supported by Responder.Responder returns information about failed transactions using the result element of the response message header.</wsdl:documentation><wsdl:input message="tns:GetMethodsSoapIn"/><wsdl:output message="tns:GetMethodsSoapOut"/></wsdl:operation><wsdl:operation name="InitiateManufacturerSpecificCommand"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">This command permits the creation of a tunnel through the AMI network to PAN devices for the purpose of delivering a manufacturer-specific capability.  The result of this message is returned asynchronously in a ManufacturerSpecificCommandNotification method.</wsdl:documentation><wsdl:input message="tns:InitiateManufacturerSpecificCommandSoapIn"/><wsdl:output message="tns:InitiateManufacturerSpecificCommandSoapOut"/></wsdl:operation><wsdl:operation name="InitiatePANCommissioning"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">This command permits the commissioning of a new PAN in preparation of registering PANDevices. The timeout parameter is a time duration during which permission to commission the PAN is active; after this time permission to commission will expire. The result of the command is returned to the URL specified in the responseURL parameter using the asynchronous PANCommissionNotification, which is linked to this method call using the transactionID.Receiver returns information about failed transactions using the result element of the response message header.</wsdl:documentation><wsdl:input message="tns:InitiatePANCommissioningSoapIn"/><wsdl:output message="tns:InitiatePANCommissioningSoapOut"/></wsdl:operation><wsdl:operation name="InitiatePANPricing"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Publisher notifies subscriber to set the price structure on a PAN. The price tiers are set for the PAN and to all devices to which the price applies.  This method is linked with a subsequent PricingTiersChangedNotification that indicates the result of this method.</wsdl:documentation><wsdl:input message="tns:InitiatePANPricingSoapIn"/><wsdl:output message="tns:InitiatePANPricingSoapOut"/></wsdl:operation><wsdl:operation name="InitiatePANPricingRequests"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Requester requests the current price structure on a PAN.  The requested information is returned asynchronously via a PANPricingNotification method.</wsdl:documentation><wsdl:input message="tns:InitiatePANPricingRequestsSoapIn"/><wsdl:output message="tns:InitiatePANPricingRequestsSoapOut"/></wsdl:operation><wsdl:operation name="InitiatePANRegistration"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Requester asks PAN server to initiate and manage the registration of a new PAN device. After this communication, the PANDevice should be able to join the PAN if it has the appropriate security credentials. The timeout parameter is a time duration during which permission to pair with the PAN is active; after this time permission to pair will expire. The PAN function publishes the results of the device registration asynchronously using a PANRegistrationNotification to the URL specified in the responseURL calling parameter and references the transactionID specified to link the transaction to this Initiate request.Receiver returns information about failed transactions using the result element of the response message header.</wsdl:documentation><wsdl:input message="tns:InitiatePANRegistrationSoapIn"/><wsdl:output message="tns:InitiatePANRegistrationSoapOut"/></wsdl:operation><wsdl:operation name="InitiatePANRegistrationStatusRequests"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Requester asks PAN server to initiate and manage the request for the registration statuses of one or more PAN devices. The PAN function publishes the registration status of the devices asynchronously using a PANRegistrationNotification to the URL specified in the responseURL calling parameter and references the transactionID specified to link the transaction to this Initiate request.Receiver returns information about failed transactions using the result element of the response message header.</wsdl:documentation><wsdl:input message="tns:InitiatePANRegistrationStatusRequestsSoapIn"/><wsdl:output message="tns:InitiatePANRegistrationStatusRequestsSoapOut"/></wsdl:operation><wsdl:operation name="InitiatePremisesDisplayBillingMessages"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Sender notifies receiver that billing messages should be shown on the associated premisesDisplay.  The result of this method is returned asynchronously in a PremisesDisplayMessageStatusNotification method.</wsdl:documentation><wsdl:input message="tns:InitiatePremisesDisplayBillingMessagesSoapIn"/><wsdl:output message="tns:InitiatePremisesDisplayBillingMessagesSoapOut"/></wsdl:operation><wsdl:operation name="InitiatePremisesDisplayCapabilitySettings"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Sender notifies receiver of capabilities of the associated premisesDisplay(s). The result of this method is returned asynchronously in a PremisesDisplayCapabilitySettingsNotification method.</wsdl:documentation><wsdl:input message="tns:InitiatePremisesDisplayCapabilitySettingsSoapIn"/><wsdl:output message="tns:InitiatePremisesDisplayCapabilitySettingsSoapOut"/></wsdl:operation><wsdl:operation name="InitiatePremisesDisplayMessages"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Sender notifies receiver that messages should be shown on the associated premisesDisplay(s).  The result of this method is returned asynchronously in a PremisesDisplayMessageStatusNotification method.</wsdl:documentation><wsdl:input message="tns:InitiatePremisesDisplayMessagesSoapIn"/><wsdl:output message="tns:InitiatePremisesDisplayMessagesSoapOut"/></wsdl:operation><wsdl:operation name="InitiateThermostatConfigurationRequests"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Requester requests the current configuration on a thermostat. The configuration is returned using the asynchronous ThermostatConfigurationNotification,  which is linked to this method call using the transactionID and should be returned to the URL specified in the responseURL. The thermostat configuration is carried in the thermostatCurrentConfiguration object.</wsdl:documentation><wsdl:input message="tns:InitiateThermostatConfigurationRequestsSoapIn"/><wsdl:output message="tns:InitiateThermostatConfigurationRequestsSoapOut"/></wsdl:operation><wsdl:operation name="InitiateThermostatConfigurations"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Requester requests the PAN Communications server manage the configuration of a programmable controlling thermostat. The result of this request is returned using the asynchronous ThermostatConfigurationNotification, which is linked to this method call using the transactionID and should be returned to the URL specified in the responseURL.</wsdl:documentation><wsdl:input message="tns:InitiateThermostatConfigurationsSoapIn"/><wsdl:output message="tns:InitiateThermostatConfigurationsSoapOut"/></wsdl:operation><wsdl:operation name="InitiateThermostatScheduleRequests"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Requester requests the current heating and/or cooling schedule on a thermostat. The thermostat schedule is returned using the asynchronous ThermostatScheduleNotification, which is linked to this method call using the transactionID and should be returned to the URL specified in the responseURL.</wsdl:documentation><wsdl:input message="tns:InitiateThermostatScheduleRequestsSoapIn"/><wsdl:output message="tns:InitiateThermostatScheduleRequestsSoapOut"/></wsdl:operation><wsdl:operation name="InitiateThermostatSchedules"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Requester requests that subscriber establish a heating and/or cooling schedule for a thermostat. The asynchronous return is a ThermostatScheduleNotification, which is linked to this method call using the transactionID and should be returned to the URL specified in the responseURL.</wsdl:documentation><wsdl:input message="tns:InitiateThermostatSchedulesSoapIn"/><wsdl:output message="tns:InitiateThermostatSchedulesSoapOut"/></wsdl:operation><wsdl:operation name="InsertInPANDeviceGroup"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Sender notifies receiver to insert PANDevices into an existing PAN device group on the server.Receiver returns information about failed transactions using the result element of the response message header.</wsdl:documentation><wsdl:input message="tns:InsertInPANDeviceGroupSoapIn"/><wsdl:output message="tns:InsertInPANDeviceGroupSoapOut"/></wsdl:operation><wsdl:operation name="InsertInPremisesDisplayGroup"><wsdl:documentation xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">Sender notifies receiver to insert premises display(s), specified by providing their objectIDs, in an existin