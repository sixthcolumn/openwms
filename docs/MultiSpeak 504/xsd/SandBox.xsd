	<xs:import namespace="http://www.multispeak.org/V5.0/enumerations" schemaLocation="mspEnumerations.xsd"/>
	<xs:import namespace="http://www.multispeak.org/V5.0/commonTypes" schemaLocation="mspCommonTypes.xsd"/>
	<xs:import namespace="http://www.multispeak.org/V5.0/primitives" schemaLocation="mspPrimitives.xsd"/>
	<xs:import namespace="http://www.multispeak.org/V5.0" schemaLocation="MultiSpeak.xsd"/>
	<xs:complexType name="asFound">
		<xs:complexContent>
			<xs:extension base="meterConditions"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="asLeft">
		<xs:annotation>
			<xs:documentation>Conditions as meter left after test.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="meterConditions"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="damageAssessment">
		<xs:annotation>
			<xs:documentation>This is a field damage assessment report.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="msp:mspObject">
				<xs:sequence>
					<xs:element name="estimated" type="xs:float" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Estimated time to repair in hours</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="eventID" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Event ID if part of an event or outage</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="jobNumber" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The jobNumber associated with this assessment.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="expiresOn" type="prim:mspDateTime" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="damageAssessments">
		<xs:sequence>
			<xs:element name="damageAssessment" type="damageAssessment" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="damageAssessmentLocation">
		<xs:complexContent>
			<xs:extension base="msp:mspPointObject">
				<xs:sequence>
					<xs:element name="address" type="com:address" minOccurs="0" maxOccurs="1"/>
					<xs:element name="locationOffset" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>
Allows the collector to specify that the location is a offset of the collected location. Examples. 5 spans north, 2 houses east, or 1 tap north.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="damageAssessments" type="damageAssessments" minOccurs="0" maxOccurs="1"/>
					<xs:element name="attachments" type="msp:attachments" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="blinkAlarm">
		<xs:complexContent>
			<xs:extension base="mspAlarm">
				<xs:sequence>
					<xs:element name="countedValue" type="xs:integer" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Accumulated value being reported.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ceilingValue" type="xs:integer" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>This is the value beyond which the accumulator will roll over or latch at maximum.  </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="latchesAtMaximum" type="xs:boolean" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>This is true for an accumulator that does not rollover upon reaching ceiling value.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="meterTests">
		<xs:complexContent>
			<xs:extension base="msp:mspReferable">
				<xs:sequence>
					<xs:element name="electricMeterTests" type="electricMeterTests" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>This is an array of tests on electricMeters.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="generatingUnits">
		<xs:annotation>
			<xs:documentation>The generating units associated with this DER.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="generatingUnitRef" type="com:objectRef" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="materialRequisitionCancellation">
		<xs:annotation>
			<xs:documentation>This is the payload of the CancelMaterialRequisition method.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="msp:mspReferable">
				<xs:sequence>
					<xs:element name="requisitionID" type="com:objectID" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The objectID of the material requisition that is to be canceled.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="workItemReference" type="com:objectRef" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>This is a pointer to the work item to which this material was to be committed.  The work item may be a workOrder, a design, a serviceOrder or any other item of work.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="designReference" type="com:nounUnspecifiedRef" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>If the workItemReference is a workOrder, this is a reference to the specific design on that workOrder to which the material was to have been committed.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="employeeTimeRecord">
		<xs:complexContent>
			<xs:extension base="msp:mspObject">
				<xs:sequence>
					<xs:element name="employeeID" type="com:objectID" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Unique identifier for an employee.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="employeeName" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="hoursWorked" type="xs:float" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Total hours worked by the employee during this time period.(Exactly 1 per employee)|.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="employeeTimeRecordList">
		<xs:sequence>
			<xs:element name="employeeTimeRecord" type="employeeTimeRecord" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A list of all employee time records.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="endDeviceShipment">
		<xs:complexContent>
			<xs:extension base="msp:mspObject">
				<xs:sequence>
					<xs:element name="utility" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="manufacturer" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="poReferenceNumber" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Purchase order number.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="shippingTicketNumber" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="recipient" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Party to whom the shipment is sent.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="shippedDate" type="prim:mspDateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Date this shipment was sent.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="shippedToAddress" type="com:address" minOccurs="0" maxOccurs="1"/>
					<xs:element name="receivedDate" type="prim:mspDateTime" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Date this shipment was received.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="receivedGasMeters" type="msp:receivedGasMeters" minOccurs="0" maxOccurs="1"/>
					<xs:element name="receivedElectricMeters" type="msp:receivedElectricMeters" minOccurs="0" maxOccurs="1"/>
					<xs:element name="receivedLoadManagementDevices" type="msp:receivedLoadManagementDevices" minOccurs="0" maxOccurs="1"/>
					<xs:element name="receivedCDDevices" type="msp:receivedCDDevices" minOccurs="0" maxOccurs="1"/>
					<xs:element name="receivedWaterMeters" type="msp:receivedWaterMeters" minOccurs="0" maxOccurs="1"/>
					<xs:element name="receivedModules" type="msp:receivedModules" minOccurs="0" maxOccurs="1"/>
					<xs:element name="receivedPremisesDisplays" type="msp:receivedPremisesDisplays" minOccurs="0" maxOccurs="1"/>
					<xs:element name="receivedPropaneMeters" type="msp:receivedPropaneMeters" minOccurs="0" maxOccurs="1"/>
					<xs:element name="transponderIDRange" type="msp:transponderIDRange" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Range of idnetifiers for communications modules (transponders).</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DERGroup">
		<xs:annotation>
			<xs:documentation>A grouping of Distributed Energy Resource (DER) facilities.  The DERGroupName SHALL be carried in the DERGroup.primaryIdentifier.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="msp:mspObject">
				<xs:sequence>
					<xs:element name="DERIDs" type="DERIDs" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DERIDs">
		<xs:annotation>
			<xs:documentation>Pointers to the distributed energy resources contained in this group.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="DERID" type="DERID" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DER">
		<xs:annotation>
			<xs:documentation>A distributed energy resource.  The name of the DER SHALL be carried in the DER.primaryIdentifier.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="msp:mspPointObject">
				<xs:sequence>
					<xs:element name="DERType" type="DERType" minOccurs="1" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The type of DER this plant represents.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="contactInfo" type="com:contactInfo" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Contact information for this DER.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="serviceAgreements" type="msp:serviceAgreements" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Interconnection and or rate agreement between this DER and others such as the interconnecting utility. </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="organizationAssociations" type="msp:organizationAssociations" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Associations between this DER and organizations.  One such association is the owner of the DER.  Others might be operators, maintainers, etc.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="realPowerRatings" type="realPowerRatings" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Real power ratings for this DER.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="reactivePowerRatings" type="reactivePowerRatings" minOccurs="0" maxOccurs="1"/>
					<xs:element name="apparentPowerRatings" type="apparentPowerRatings" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>ApparentPowerRatings for DER.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="storagePowerRating" type="com:realEnergy" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The usable energy storage capability of the DER.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="notificationTime" type="com:duration" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The notification time required before DER can start operation.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DERRealPowerRampRate" type="realPowerRampRate" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The real power ramp rate for the DER.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="generatingUnits" type="generatingUnits" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="DERKind">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Unknown"/>
			<xs:enumeration value="BatteryStorage"/>
			<xs:enumeration value="Biogas"/>
			<xs:enumeration value="Biomass"/>
			<xs:enumeration value="Cogeneration"/>
			<xs:enumeration value="CombinationDER"/>
			<xs:enumeration value="CompressedAir"/>
			<xs:enumeration value="Flywheel"/>
			<xs:enumeration value="FuelCell"/>
			<xs:enumeration value="Hydro"/>
			<xs:enumeration value="Photovoltaic"/>
			<xs:enumeration value="Thermal"/>
			<xs:enumeration value="Wind"/>
			<xs:enumeration value="Other"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="DERType">
		<xs:annotation>
			<xs:documentation>Type of DER that this data instance represents.</xs:documentation>
		</xs:annotation>
		<xs:simpleContent>
			<xs:extension base="DERKind">
				<xs:attribute name="otherKind" type="enum:otherKind" use="optional">
					<xs:annotation>
						<xs:documentation>If no acceptable hard enumeration value is available, the choice "Other" SHALL be used and the extended enumeration SHALL be included in this attribute.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="realPowerRatings">
		<xs:complexContent>
			<xs:extension base="com:mspExtensible">
				<xs:sequence>
					<xs:element name="realPowerRating" type="realPowerRating" minOccurs="1" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="reactivePowerRatings">
		<xs:complexContent>
			<xs:extension base="com:mspExtensible">
				<xs:sequence>
					<xs:element name="reactivePowerRating" type="reactivePowerRating" minOccurs="1" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="inspection">
		<xs:complexContent>
			<xs:extension base="msp:mspWork">
				<xs:sequence>
					<xs:element name="elementStatus" type="xs:string" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>The status of the inspected item.  For instance: Okay, Needs Attention, Needs Immediate Attention, Not Functional, or Okay as Left.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="elementCondition" type="xs:integer" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>Quality factor, expressed in percent.  For instance 100 might been new, 0 might mean no life remaining.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="serviceOrderID" type="com:objectID" minOccurs="0" maxOccurs="1">
						<xs:annotation>
							<xs:documentation>A pointer to the service order this inspection was done under.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="observationList" type="observationList" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="itemMaintained">
		<xs:sequence>
			<xs:element name="itemIdentifier" type="com:objectRef" minOccurs="0" maxOccurs="1"/>
			<xs:element name="maintenanceCode" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="conditionBefore" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="conditionAfter" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="remarks" type="xs:string" minOccurs="0" maxOccurs="1"/>
			<xs:element name="measurements" type="msp:measurements" minOccurs="0" maxOccurs="1"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="itemsMaintained">
		<xs:sequence>
			<xs:element name="itemMaintained" type="itemMaintained" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="jobWorked">
		<xs:sequence>
			<xs:element name="woNumber" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Work order number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="jobNumber" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Job number.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="jobDescription" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Description of job.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="jobHours" type="xs:float" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Hours worked on a specific job.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="jobWorkedList">
		<xs:sequence>
			<xs:element name="jobWorked" type="jobWorked" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>A list of all jobs worked.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="apparentPowerRatings">
		<xs:complexContent>
			<xs:extension base="com:mspExtensible">
				<xs:sequence>
					<xs:element name="apparentPowerRating" type="apparentPowerRating" minOccurs="1" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="realPowerRating">
		<xs:sequence>
			<xs:element name="realPower" type="com:realPower" minOccurs="1" maxOccurs="1"/>
			<xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Description of rating type, such as DeliveredToGrid, ReceivedFromGrid, etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="reactivePowerRating">
		<xs:sequence>
			<xs:element name="reactivePower" type="com:reactivePower" minOccurs="1" maxOccurs="1"/>
			<xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Description of rating type, such as DeliveredToGrid, ReceivedFromGrid, etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="apparentPowerRating">
		<xs:sequence>
			<xs:element name="apparentPower" type="com:apparentPower" minOccurs="1" maxOccurs="1"/>
			<xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Description of rating type, such as DeliveredToGrid, ReceivedFromGrid, etc.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="realPowerRampRate">
		<xs:simpleContent>
			<xs:extension base="prim:floatType">
				<xs:attribute name="realPowerUnits" type="enum:realPowerUnits" use="optional">
					<xs:annotation>
						<xs:documentation>Units of real power associated with the ramp rate.  For instance, if the ramp rate is 10 MW/hour, then the real power units are MW.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="timeUnits" type="enum:timeUnits" use="optional">
					<xs:annotation>
						<xs:documentation>The time units associated with the ramp rate.  For instance, if the ramp rate is 10 MW/Hour, then the time units are hours.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="maintenanceDataSet">
		<xs:complexContent>
			<xs:extension base="msp:mspObject">
				<xs:sequence>
					<xs:element name="maintenanceDate" type="prim:mspDateTime" minOccurs="0" maxOccurs="1"/>
					<xs:element name="description" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="itemsMaintained" type="itemsMaintained" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
