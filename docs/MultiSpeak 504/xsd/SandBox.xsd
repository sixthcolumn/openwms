	<xs:complexType name="transactiveSignal">
		<xs:annotation>
			<xs:documentation>This is a signal that is designed to be sent as part of a communication between Transactive Control nodes.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TCSchemaVersion" type="TCSchemaVersion" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Transactive Control schema version.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="XMLCreationDateTime" type="xs:dateTime" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The UTC date and time that this XML transactive signal instance was created.  </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TCSourceID" type="TCSourceID" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The sender ID of this transactive signal. This is the ID of the transactive node OR the system location at which this signal was formulated. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TCTargetID" type="TCTargetID" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>The receiver ID of this transactive signal. This is the ID of the transactive node OR the system location at which this signal was sent.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="messageChoice" type="messageChoice" minOccurs="1" maxOccurs="1"/>
		</xs:sequence>
		<xs:attribute name="transactiveSignalStatus" type="transactiveSignalModeType" use="required">
			<xs:annotation>
				<xs:documentation>Status of the transactive node.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:simpleType name="transactiveSignalModeType">
		<xs:annotation>
			<xs:documentation>Operating states of a transactive node.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Invalid"/>
			<xs:enumeration value="Operational"/>
			<xs:enumeration value="Testing"/>
			<xs:enumeration value="Simulated"/>
			<xs:enumeration value="Startup"/>
			<xs:enumeration value="Other"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="TCInterval">
		<xs:annotation>
			<xs:documentation>This is the data for this transactive incentive</xs:documentation>
		</xs:annotation>
		<xs:attribute name="startDateTime" type="xs:dateTime" use="required">
			<xs:annotation>
				<xs:documentation>Interval date and start time in UTC format.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="value" type="xs:float" use="required">
			<xs:annotation>
				<xs:documentation>Energy cost future value estimate for this interval.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="confidenceLevel" type="confidenceLevelType" use="required">
			<xs:annotation>
				<xs:documentation>Confidence in the future value estimate for this interval.  If a calculation is used, the value must be a positive, real number with a large number indicating less confidence. If no confidence is calculated, the value is to be -1.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:simpleType name="restrictedFloat">
		<xs:annotation>
			<xs:documentation>Float restricted to be a positive, real number.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:float">
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tokenEnumeration">
		<xs:restriction base="xs:string">
			<xs:enumeration value="-1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="confidenceLevelType">
		<xs:annotation>
			<xs:documentation>Be able to union two choices:  a positive, real value OR "-1" for not applicable.</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="sandbox:restrictedFloat sandbox:stringToken"/>
	</xs:simpleType>
	<xs:simpleType name="stringToken">
		<xs:restriction base="sandbox:tokenEnumeration"/>
	</xs:simpleType>
	<xs:complexType name="messageChoice">
		<xs:choice>
			<xs:element name="transactiveIncentiveSignal" type="transactiveIncentiveSignal" minOccurs="1" maxOccurs="1"/>
			<xs:element name="transactiveFeedbackSignal" type="transactiveFeedbackSignal" minOccurs="1" maxOccurs="1"/>
		</xs:choice>
	</xs:complexType>
	<xs:complexType name="TCIntervals">
		<xs:sequence>
			<xs:element name="TCInterval" type="TCInterval" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="TCSchemaVersion">
		<xs:annotation>
			<xs:documentation>Transactive signal schema version.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[1-9]+[0-9]*\.[0-9]+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TFSValueUnitType">
		<xs:annotation>
			<xs:documentation>A list of allowable power units.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="kW"/>
			<xs:enumeration value="MW"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="totalIntervals">
		<xs:annotation>
			<xs:documentation>The total number of intervals (i.e., how many TCInterval blocks.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TCSourceID">
		<xs:annotation>
			<xs:documentation>The sender ID of this transactive signal. This is the ID of the transactive node OR the system location at which this signal was formulated. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TCTargetID">
		<xs:annotation>
			<xs:documentation>The receiver ID of this transactive signal. This is the ID of the transactive node OR the system location at which this signal was sent.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
