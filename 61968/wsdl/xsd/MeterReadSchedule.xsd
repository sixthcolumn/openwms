							<xs:annotation>
								<xs:documentation>Any free text that name the object.
								</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="NameType" minOccurs="0" maxOccurs="1"
							sawsdl:modelReference="http://iec.ch/TC57/2010/CIM-schema-cim15#Name.NameType">
							<xs:annotation>
								<xs:documentation>Type of this name.</xs:documentation>
							</xs:annotation>
							<xs:complexType
								sawsdl:modelReference="http://iec.ch/TC57/2010/CIM-schema-cim15#NameType">
								<xs:sequence>
									<xs:element name="description" minOccurs="0"
