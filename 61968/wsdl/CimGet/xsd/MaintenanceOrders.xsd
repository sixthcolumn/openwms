						<xs:element name="start" type="xs:dateTime" minOccurs="0" maxOccurs="1" sawsdl:modelReference="http://iec.ch/TC57/CIMgeneric#DateTimeInterval.start">
							<xs:annotation>
								<xs:documentation>Start date and time of this interval.</xs:documentation>
							</xs:annotation>
						</xs:element>
