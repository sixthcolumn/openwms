<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://iec.ch/TC57/2015/ServiceOrdersMessage" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msg="http://iec.ch/TC57/2011/schema/message" xmlns:obj="http://iec.ch/TC57/2015/ServiceOrders#" targetNamespace="http://iec.ch/TC57/2015/ServiceOrdersMessage" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0.0">
  <!-- Base Message Definitions -->
  <xs:import namespace="http://iec.ch/TC57/2011/schema/message" schemaLocation="Message.xsd"/>
  <!-- CIM Information Object Definition -->
  <xs:import namespace="http://iec.ch/TC57/2015/ServiceOrders#" schemaLocation="ServiceOrders.xsd"/>
  <!-- PayloadType Definition -->
  <xs:complexType name="ServiceOrdersPayloadType">
    <xs:sequence>
      <xs:element ref="obj:ServiceOrders"/>
      <xs:element name="OperationSet" type="msg:OperationSet" minOccurs="0"/>
      <xs:element name="Compressed" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>For compressed and/or binary, uuencoded payloads</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Format" type="xs:string" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Hint as to format of payload, e.g. XML, RDF, SVF, BINARY, PDF, ...</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <!-- Message Types -->
  <!-- RequestMessageType -->
  <xs:complexType name="ServiceOrdersRequestMessageType">
    <xs:sequence>
      <xs:element name="Header" type="msg:HeaderType"/>
      <xs:element name="Request" type="msg:RequestType" minOccurs="0"/>
      <xs:element name="Payload" type="tns:ServiceOrdersPayloadType" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <!-- ResponseMessageType -->
  <xs:complexType name="ServiceOrdersResponseMessageType">
    <xs:sequence>
      <xs:element name="Header" type="msg:HeaderType"/>
      <xs:element name="Reply" type="msg:ReplyType"/>
      <xs:element name="Payload" type="tns:ServiceOrdersPayloadType" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <!-- EventMessageType -->
  <xs:complexType name="ServiceOrdersEventMessageType">
    <xs:sequence>
      <xs:element name="Header" type="msg:HeaderType"/>
      <xs:element name="Payload" type="tns:ServiceOrdersPayloadType" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>
  <!-- FaultMessageType -->
  <xs:complexType name="ServiceOrdersFaultMessageType">
    <xs:sequence>
      <xs:element name="Reply" type="msg:ReplyType"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="CreateServiceOrders" type="tns:ServiceOrdersRequestMessageType"/>
  <xs:element name="ChangeServiceOrders" type="tns:ServiceOrdersRequestMessageType"/>
  <xs:element name="CancelServiceOrders" type="tns:ServiceOrdersRequestMessageType"/>
  <xs:element name="CloseServiceOrders" type="tns:ServiceOrdersRequestMessageType"/>
  <xs:element name="DeleteServiceOrders" type="tns:ServiceOrdersRequestMessageType"/>
  <xs:element name="CreatedServiceOrders" type="tns:ServiceOrdersEventMessageType"/>
  <xs:element name="ChangedServiceOrders" type="tns:ServiceOrdersEventMessageType"/>
  <xs:element name="CanceledServiceOrders" type="tns:ServiceOrdersEventMessageType"/>
  <xs:element name="ClosedServiceOrders" type="tns:ServiceOrdersEventMessageType"/>
  <xs:element name="DeletedServiceOrders" type="tns:ServiceOrdersEventMessageType"/>
  <xs:element name="ServiceOrdersResponseMessage" type="tns:ServiceOrdersResponseMessageType"/>
  <xs:element name="ServiceOrdersFaultMessage" type="tns:ServiceOrdersFaultMessageType"/>
</xs:schema>
