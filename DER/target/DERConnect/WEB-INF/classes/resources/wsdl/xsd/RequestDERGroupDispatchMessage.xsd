<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://iec.ch/TC57/2011/DERGroupDispatchMessage"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msg="http://iec.ch/TC57/2011/schema/message"
	xmlns:obj="http://www.epri.com/2013/DERGroupDispatch#" 
	targetNamespace="http://iec.ch/TC57/2011/DERGroupDispatchMessage"
	elementFormDefault="qualified" attributeFormDefault="unqualified"
	version="1.0.0">
	<!-- Base Message Definitions -->
	<xs:import namespace="http://iec.ch/TC57/2011/schema/message"
		schemaLocation="Message.xsd" />
	<!-- CIM Information Object Definition -->
	<xs:import namespace="http://www.epri.com/2013/DERGroupDispatch#" 
		schemaLocation="DERGroupDispatch.xsd" />
	<!-- PayloadType Definition -->
	<xs:complexType name="DERGroupDispatchPayloadType">
		<xs:sequence>
			<xs:element ref="obj:DERGroupDispatches" />
			<xs:element name="OperationSet" type="msg:OperationSet"
				minOccurs="0" />
			<xs:element name="Compressed" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>For compressed and/or binary, uuencoded
						payloads</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Format" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Hint as to format of payload, e.g. XML, RDF, SVF,
						BINARY,
						PDF, ...</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- Message Types -->
	<!-- RequestMessageType -->
	<xs:complexType name="DERGroupDispatchRequestMessageType">
		<xs:sequence>
			<xs:element name="Header" type="msg:HeaderType" />
			<xs:element name="Request" type="msg:RequestType"
				minOccurs="0" />
			<xs:element name="Payload"
				type="tns:DERGroupDispatchPayloadType" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<!-- ResponseMessageType -->
	<xs:complexType name="DERGroupDispatchResponseMessageType">
		<xs:sequence>
			<xs:element name="Header" type="msg:HeaderType" />
			<xs:element name="Reply" type="msg:ReplyType" />
			<xs:element name="Payload"
				type="tns:DERGroupDispatchPayloadType" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<!-- EventMessageType -->
	<xs:complexType name="DERGroupDispatchEventMessageType">
		<xs:sequence>
			<xs:element name="Header" type="msg:HeaderType" />
			<xs:element name="Payload"
				type="tns:DERGroupDispatchPayloadType" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	<!-- FaultMessageType -->
	<xs:complexType name="DERGroupDispatchFaultMessageType">
		<xs:sequence>
			<xs:element name="Reply" type="msg:ReplyType" />
		</xs:sequence>
	</xs:complexType>
	<xs:element name="CreateDERGroupDispatch"
		type="tns:DERGroupDispatchRequestMessageType" />
	<xs:element name="ChangeDERGroupDispatch"
		type="tns:DERGroupDispatchRequestMessageType" />
	<xs:element name="CancelDERGroupDispatch"
		type="tns:DERGroupDispatchRequestMessageType" />
	<xs:element name="CloseDERGroupDispatch"
		type="tns:DERGroupDispatchRequestMessageType" />
	<xs:element name="DeleteDERGroupDispatch"
		type="tns:DERGroupDispatchRequestMessageType" />
	<xs:element name="CreatedDERGroupDispatch"
		type="tns:DERGroupDispatchEventMessageType" />
	<xs:element name="ChangedDERGroupDispatch"
		type="tns:DERGroupDispatchEventMessageType" />
	<xs:element name="CanceledDERGroupDispatch"
		type="tns:DERGroupDispatchEventMessageType" />
	<xs:element name="ClosedDERGroupDispatch"
		type="tns:DERGroupDispatchEventMessageType" />
	<xs:element name="DeletedDERGroupDispatch"
		type="tns:DERGroupDispatchEventMessageType" />
	<xs:element name="DERGroupDispatchResponseMessage"
		type="tns:DERGroupDispatchResponseMessageType" />
	<xs:element name="DERGroupDispatchFaultMessage"
		type="tns:DERGroupDispatchFaultMessageType" />
</xs:schema>