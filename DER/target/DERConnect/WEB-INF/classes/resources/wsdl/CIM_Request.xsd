	<xs:element name="GetDERGroupCapabilityPayload" type="ns:getDERGroupCapabilityRequest" />
	<xs:element name="GetDERGroupCapabilityResponse"
		type="ns5:GetDERGroupCapabilityResponseMessageType" />
